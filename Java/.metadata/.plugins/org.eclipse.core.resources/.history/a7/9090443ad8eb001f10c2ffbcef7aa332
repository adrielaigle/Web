package avl;

public class Main {
    public static void main(String[] args) {
        AVL arvore = new AVL(30);

        // Inserções
        arvore.insere(20);
        arvore.insere(40);
        arvore.insere(10);
        arvore.insere(25);
        arvore.insere(35);
        arvore.insere(50);
        arvore.insere(5);
        arvore.insere(15);

        System.out.println("Árvore em pré-ordem (antes da remoção):");
        System.out.println(arvore.preOrdem());

        // Remoção de alguns nós
        arvore.remove(10);
        arvore.remove(40);

        System.out.println("\nÁrvore em pré-ordem (após remoção):");
        System.out.println(arvore.preOrdem());
    }
}
